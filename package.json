{
    "name": "skychat",
    "private": false,
    "version": "0.0.1",
    "description": "",
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "scripts": {
        "setup": "bash scripts/setup.sh",
        "clean:client": "rm -rf dist",
        "clean:server": "rm -rf build",
        "clean": "npm run clean:client && npm run clean:server",
        "build:client": "npm run clean:client && ./node_modules/gulp/bin/gulp.js build-client",
        "build:server": "npm run clean:server && ./node_modules/gulp/bin/gulp.js build-server",
        "build": "npm run clean && ./node_modules/gulp/bin/gulp.js build",
        "dev:client": "npm run clean:client && ./node_modules/gulp/bin/gulp.js watch-client",
        "dev:server": "npm run clean:server && ./node_modules/gulp/bin/gulp.js watch-server",
        "kill": "ps aux | grep 'node --max-old-space-size=8192 build/server.js' | awk '{print $2}' | xargs kill -9",
        "dev": "npm run clean && ./node_modules/gulp/bin/gulp.js watch",
        "start": "npm run build && node build/server.js"
    },
